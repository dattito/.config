ralt - 1 : open -a iTerm
ralt + shift - 1 : open -a Slack
ralt - 2 : open -a "Google Chrome"
ralt + shift - 2 : open -a Safari
ralt + rcmd - 2 : open -a Firefox
ralt - 3 : open -a Music
ralt + shift - 3 : open -a "Microsoft Teams"
ralt - 4 : open -a Settings
ralt + shift - 4 : open -a "Messages"
ralt - 5 : open -a "Visual Studio Code"
ralt - 6 : open -a "Microsoft Outlook"
ralt - 7 : open -a Docker
lshift + ralt - q : osascript -e 'tell application "Finder" to sleep'

lalt + shift - m : /Applications/Privileges.app/Contents/Resources/PrivilegesCLI --add
lalt - m : /Applications/Privileges.app/Contents/Resources/PrivilegesCLI --remove

ralt - s : yabai -m window --focus stack.prev || yabai -m window --focus stack.last
ralt - d : yabai -m window --focus stack.next || yabai -m window --focus stack.first

#ctrl - z : yabai -m window --focus west || yabai -m space --focus prev || yabai -m space --focus last
#ctrl - u : yabai -m window --focus south
#ctrl - i : yabai -m window --focus north
#ctrl - o : yabai -m window --focus east || yabai -m space --focus next || yabai -m space --focus first

#ctrl + shift - z : yabai -m window --warp west || (yabai -m window --space prev && yabai -m space --focus prev) || (yabai -m window --space last && yabai -m space --focus last)
#ctrl + shift - u : yabai -m window --warp south
#ctrl + shift - i : yabai -m window --warp north
#ctrl + shift - o : yabai -m window --warp east || (yabai -m window --space next && yabai -m space --focus next) || (yabai -m window --space first && yabai -m space --focus first)

#alt - i : yabai -m space --focus prev || yabai -m space --focus last
#alt - o : yabai -m space --focus next || yabai -m space --focus first

#ctrl + alt - i : yabai -m window --space prev && yabai -m space --focus prev
#ctrl + alt - o : yabai -m window --space next && yabai -m space --focus next

#alt + shift - d : yabai -m space --destroy
#alt + shift - n : yabai -m space --create && yabai -m window --space next && yabai -m space --focus next

ralt - a : yabai -m window --display prev || yabai -m window --display last; yabai -m display --focus prev || yabai -m display --focus last
ralt - f : yabai -m window --display next || yabai -m window --display first; yabai -m display --focus next || yabai -m display --focus first

